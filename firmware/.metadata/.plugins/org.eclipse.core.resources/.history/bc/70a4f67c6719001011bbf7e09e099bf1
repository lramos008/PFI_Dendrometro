#ifndef FS_INTERFACE_H
#define FS_INTERFACE_H
// ==========================[ Includes ]===============================

// ==========================[ Typedefs ]===============================
typedef enum{
	FS_OK = 0,
	FS_ERR_INIT,
	FS_ERR_MOUNT,
	FS_ERR_UNMOUNT,
	FS_ERR_OPEN,
	FS_ERR_CLOSE,
	FS_ERR_WRITE,
	FS_ERR_READ,
	FS_ERR_UNKNOWN
}tFSResult;

typedef enum{
	FS_MODE_READ = 0,
	FS_MODE_WRITE,
	FS_MODE_APPEND,
	FS_MODE_CREATE_NEW,
	FS_MODE_CREATE_ALWAYS
}tFSOpenMode;


#endif //FS_INTERFACE_H
