// ==========================[ Includes ]===============================
#include "fifo.h"
// ======================[ Public functions ]===============================
void fifoInit(fifoHandler_t *f, uint8_t *buffer, uint16_t size){
	f->buffer = buffer;
	f->size = size;
	f->head = 0;
	f->tail = 0;
	return;
}

bool fifoPush(fifoHandler_t *f, uint8_t data){
	uint16_t nextHead = (f->head + 1) % f->size;
	if(nextHead == f->tail){
		return false;
	}
}
