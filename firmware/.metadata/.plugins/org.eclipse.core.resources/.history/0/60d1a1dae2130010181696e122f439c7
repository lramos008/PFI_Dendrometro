#ifndef DHT22_DRIVER_H
#define DHT22_DRIVER_H
// ==========================[ Includes ]===============================
#include "delay_us.h"

// ==========================[ Typedefs ]===============================
typedef enum{
	DHT22_SENSOR_PRESENT = 0,
	DHT22_SENSOR_NOT_PRESENT
}tDht22State;

// ==========================[ Structures ]=============================
typedef struct{
	uint8_t integralRH;
	uint8_t decimalRH;
	uint8_t integralTemp;
};
#endif //DHT22_DRIVER_H
